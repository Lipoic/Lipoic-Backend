{
  "name": "lipoic-backend",
  "version": "1.0.0",
  "description": "The backend repository for lipoic, built with node.js + typescript + express.js.",
  "main": "dist/src/index.js",
  "repository": "https://github.com/Lipoic/Lipoic-Backend.git",
  "author": "Lipoic backend team",
  "license": "GPL-3.0",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "concurrently \"tsc -w\" \"tsc-alias -w\" \"nodemon dist/src/index.js\"",
    "start": "yarn build && node dist/src/index.js",
    "lint": "eslint src/**/*.ts --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "mongoose": "^6.6.0"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/mongoose": "^5.11.97",
    "@types/node": "18.7.16",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vitest/coverage-c8": "^0.23.2",
    "concurrently": "7.4.0",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "nodemon": "2.0.19",
    "prettier": "2.7.1",
    "supertest": "^6.2.4",
    "tsc-alias": "1.7.0",
    "typescript": "4.8.3",
    "vitest": "^0.23.2"
  }
}
